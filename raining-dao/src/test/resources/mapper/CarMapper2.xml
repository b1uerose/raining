<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiao.raining.test.mapper.CarMapper">
    <select id="queryCarById2" resultType="com.xiao.raining.vo.car.CarVO">
        select * from bd_car where id = #{id};
    </select>

    <insert id="insertCar2">
        insert into bd_car (vcardno, vbrand, nguideprice, dproducedate, vcartype) VALUES (#{vcardno}, #{vbrand}, #{nguideprice}, #{dproducedate}, #{vcartype});
    </insert>
</mapper>